#include <stdio.h>
#define MAX 5

int queue[MAX];
int front = -1, rear = -1;

void insert() {
    int x;
    if ((rear + 1) % MAX == front)
        printf("Queue Overflow\n");
    else {
        printf("Enter element to insert: ");
        scanf("%d", &x);
        if (front == -1)
            front = rear = 0;
        else
            rear = (rear + 1) % MAX;
        queue[rear] = x;
        printf("Element inserted\n");
    }
}

void delete() {
    if (front == -1)
        printf("Queue Empty\n");
    else {
        printf("Deleted element: %d\n", queue[front]);
        if (front == rear)
            front = rear = -1;
        else
            front = (front + 1) % MAX;
    }
}

void display() {
    int i;
    if (front == -1)
        printf("Queue is empty\n");
    else {
        printf("Queue elements:\n");
        for (i = front; i != (rear + 1) % MAX; i = (i + 1) % MAX)
            printf("%d\n", queue[i]);
    }
}

int main() {
    int ch;
    while (1) {
        printf("\n1.Insert\n2.Delete\n3.Display\n4.Exit\nEnter your choice: ");
        scanf("%d", &ch);
        switch (ch) {
            case 1: insert(); break;
            case 2: delete(); break;
            case 3: display(); break;
            case 4: return 0;
            default: printf("Invalid choice\n");
        }
    }
}
